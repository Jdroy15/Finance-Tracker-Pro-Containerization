generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
}

model Expense {
  id          Int     @id @default(autoincrement())
  userId      Int
  amount      Float
  category    String
  description String
  date        DateTime
}
